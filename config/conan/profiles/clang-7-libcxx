[settings]
os=Linux
os_build=Linux
arch=x86_64
arch_build=x86_64
compiler=clang
compiler.version=7.0
compiler.libcxx=libc++
build_type=Release
[options]
[build_requires]
[env]
CC=/usr/bin/clang-7
CXX=/usr/bin/clang++-7
CXXFLAGS=-stdlib=libc++ -nostdinc++ -cxx-isystem /opt/llvm-7.0/include/c++/v1
LDFLAGS=-fuse-ld=lld -L /opt/llvm-7.0/lib/ -Wl,-rpath,/opt/llvm-7.0/lib/ -lc++abi
CONAN_CMAKE_GENERATOR=Ninja
